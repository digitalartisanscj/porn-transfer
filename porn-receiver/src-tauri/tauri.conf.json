{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Porn Receiver",
  "version": "0.3.0",
  "identifier": "com.alin.porn-receiver",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Porn Receiver",
        "width": 900,
        "height": 650,
        "minWidth": 700,
        "minHeight": 500,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/digitalartisanscj/porn-transfer/releases/latest/download/latest.json"
      ],
      "pubkey": ""
    }
  }
}
